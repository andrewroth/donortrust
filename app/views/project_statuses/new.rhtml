<h1>New project_status</h1>

<%= error_messages_for :project_status %>

<% form_for(:project_status, :url => project_statuses_path) do |f| %>
  <p>
    <b>Status type</b><br />
    <%= f.text_field :status_type %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', project_statuses_path %>