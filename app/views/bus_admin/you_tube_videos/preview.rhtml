<div id="you_tube_thumbnail">
    <img src="<%= @video_hash["ut_response"]["video_details"]["thumbnail_url"]%>"><br/>
</div>

<div id="you_tube_details">
    <h1><%= @video_hash["ut_response"]["video_details"]["title"]%></h1>
    <strong>From</strong>: <%= @video_hash["ut_response"]["video_details"]["author"]%><br/>
    <div align="justify">
        <%= @video_hash["ut_response"]["video_details"]["description"]%>
    </div>
</div>