<h1>New project_category</h1>

<%= error_messages_for :project_category %>

<% form_for(:project_category, :url => project_categories_path) do |f| %>
  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', project_categories_path %>